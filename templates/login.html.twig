<html>
<head>
    <title>Portale d'accesso</title>
</head>
<body>




    <div class="container">



        {{ form_start(form) }}
        <div class="my-custom-class-for-errors">
            {{ form_errors(form) }}
        </div>

        <div class="row">
            <div class="col">
                {{ form_row(form.nome) }}
            </div>
            <div class="col" id="some-custom-id">
                {{ form_row(form.cognome) }}
            </div>
        </div>
        {{ form_end(form) }}
    </div>


</body>
</html>